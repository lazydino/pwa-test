{
    "name": "MyHTML Wrapper PWA",
    "short_name": "MyHTML Wrapper",
    "start_url": "index.html",
    "display": "standalone",
    "background_color": "#ffffff",
    "icons": [
      {
        "src": "icon.png",
        "sizes": "192x192",
        "type": "image/png"
      }
    ],
    "file_handlers": [
      {
        "action": "/",
        "accept": {
            "text/*": [
                ".myhtml"
            ]
        }
        "name": "Wrap MyHTML Content"
      }
    ]
  }
  
